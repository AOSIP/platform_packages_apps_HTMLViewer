<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.android.htmlviewer">
    <original-package android:name="com.android.htmlviewer" />

    <uses-sdk android:minSdkVersion="23"
            android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application android:label="@string/app_label">
        <activity android:name="HTMLViewerActivity"
                android:label="@string/app_label"
                android:theme="@android:style/Theme.DeviceDefault">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="android.intent.action.VIEW" />
                <data android:scheme="file" />
                <data android:scheme="content" />
                <data android:mimeType="text/calendar"/>
                <data android:mimeType="text/comma-separated-values"/>
                <data android:mimeType="text/css"/>
                <data android:mimeType="text/h323"/>
                <data android:mimeType="text/html"/>
                <data android:mimeType="text/iuls"/>
                <data android:mimeType="text/mathml"/>
                <data android:mimeType="text/plain"/>
                <data android:mimeType="text/tab-separated-values"/>
                <data android:mimeType="text/text"/>
                <data android:mimeType="text/x-bibtex"/>
                <data android:mimeType="text/x-boo"/>
                <data android:mimeType="text/x-c++hdr"/>
                <data android:mimeType="text/x-c++src"/>
                <data android:mimeType="text/x-chdr"/>
                <data android:mimeType="text/x-component"/>
                <data android:mimeType="text/x-csh"/>
                <data android:mimeType="text/x-csrc"/>
                <data android:mimeType="text/x-dsrc"/>
                <data android:mimeType="text/x-haskell"/>
                <data android:mimeType="text/x-java"/>
                <data android:mimeType="text/x-literate-haskell"/>
                <data android:mimeType="text/x-moc"/>
                <data android:mimeType="text/x-pascal"/>
                <data android:mimeType="text/x-pcs-gcd"/>
                <data android:mimeType="text/x-setext"/>
                <data android:mimeType="text/x-tcl"/>
                <data android:mimeType="text/x-tex"/>
                <data android:mimeType="text/x-vcalendar"/>
                <data android:mimeType="text/x-vcard"/>
                <data android:mimeType="text/xml"/>
                <data android:mimeType="application/xhtml+xml"/>
                <data android:mimeType="application/vnd.wap.xhtml+xml"/>
            </intent-filter>
        </activity>
        <meta-data android:name="android.nfc.disable_beam_default"
                android:value="true" />
    </application>
</manifest>
